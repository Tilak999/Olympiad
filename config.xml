<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ideabox.cdgi" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Commerce Olympiad</name>
    <description>
    </description>
    <author email="tilaksasmal999@gmail.com">
        Ideabox Inc
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
